<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.beanfuse.security.model.UserCategory"
    entity-name="org.beanfuse.security.model.UserCategory" table="XTQX_YHLB_T">
    <cache usage="read-write"/>
    <id name="id" column="ID" unsaved-value="null">
      <generator class="sequence">
        <param name="sequence">SEQ_XTQX_YHLB_T</param>
      </generator>
    </id>
    <property name="name" column="LBMC" length="50" unique="true"/>
  </class>
</hibernate-mapping>
